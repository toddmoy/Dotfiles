[includeIf "gitdir:zapier/"]
  path = .gitconfig-zapier
[includeIf "gitdir:toddmoy/"]
  path = .gitconfig-personal

[core]
	editor = vim
	excludesfile = ~/.global.gitignore
[color]
	ui = true
	branch = auto
	diff = auto
	status = auto
	showbranch = auto
[github]
	user = toddmoy
[help]
  autocorrect = 1
[alias]
  a = add
  b = branch
  c = commit
  co = checkout
  l = log

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[credential]
	helper = osxkeychain
